{
  "name": "smykowski",
  "version": "1.3.4",
  "description": "*I deal with the goddamn objects so JSON doesnâ€™t have to!! I have object skills!!*",
  "keywords": [
    "json",
    "stringify",
    "parse",
    "circular",
    "reference",
    "deterministic",
    "stable",
    "encode",
    "decode",
    "serialize",
    "serialization"
  ],
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypercubed/smykowski.git"
  },
  "bugs": {
    "url": "https://github.com/hypercubed/smykowski/issues"
  },
  "homepage": "https://github.com/hypercubed/smykowski#readme",
  "author": "J. Harshbarger",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all clean build:main build:module docs",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "clean": "npm-run-all clean:dist clean:docs",
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf docs",
    "lint": "tslint --project .",
    "test:unit": "nyc --silent ava",
    "check-coverage": "nyc check-coverage --lines 100 --functions 100 --branches 100",
    "test": "npm-run-all lint build:main test:unit check-coverage",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "cov": "npm-run-all build test:unit html-coverage && opn coverage/index.html",
    "html-coverage": "nyc report --reporter=html",
    "send-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "changelog": "chg release -y && git add -A CHANGELOG.md",
    "version": "npm-run-all changelog build",
    "np": "npm-run-all build:* && np --no-cleanup",
    "docs": "typedoc ./src/index.ts --out docs --mode file --theme default"
  },
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "@types/node": "^9.4.7",
    "ava": "^0.25.0",
    "chg": "^0.3.2",
    "codecov": "^3.0.0",
    "concurrently": "^3.5.0",
    "memwatch-next": "^0.3.0",
    "np": "^2.16.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "sleep-ms": "^2.0.1",
    "tslint": "^5.0.0",
    "tslint-config-swimlane": "^3.0.2",
    "typedoc": "^0.21.0",
    "typescript": "^2.7.2"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "ava": {
    "failFast": false,
    "files": [
      "dist/main/**/*.spec.js"
    ]
  },
  "dependencies": {
    "json-pointer": "^0.6.0",
    "reflect-metadata": "^0.1.12",
    "tslib": "^1.7.1"
  },
  "directories": {
    "doc": "docs"
  }
}
